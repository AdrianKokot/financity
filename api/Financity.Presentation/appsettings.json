{
  "ConnectionStrings": {
    "Financity": "Host=<host>;Port=<port>;Database=Financity;Username=<username>;Password=<password>"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./log.log"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
